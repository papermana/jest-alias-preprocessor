{
  "name": "jest-alias-preprocessor",
  "version": "1.0.2",
  "description": "A preprocessor for Jest with support for webpack aliases",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "lint:staged": "lint-staged"
  },
  "author": "papermana",
  "license": "MIT",
  "keywords": [
    "jest",
    "preprocessor",
    "alias",
    "webpack",
    "require"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/papermana/jest-alias-preprocessor.git"
  },
  "dependencies": {
    "lodash.flatten": "^4.4.0",
    "transform-jest-deps": "^2.2.0"
  },
  "devDependencies": {
    "@papermana/eslint-config-basic": "^1.0.0",
    "eslint": "^3.12.2",
    "jest-cli": "^13.2.3",
    "lint-staged": "^3.2.4",
    "pre-commit": "^1.2.2"
  },
  "jest": {
    "testDirectoryName": "tests"
  },
  "pre-commit": "lint:staged",
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  }
}
